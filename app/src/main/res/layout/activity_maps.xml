<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".View.Maps_Activity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">
        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context="VoLap.example.appLoship.View.MapsActivity">

            <HorizontalScrollView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="520dp">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp">


                    <RelativeLayout
                        android:id="@+id/quan1_118"
                        android:layout_width="wrap_content"
                        android:layout_height="250dp"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/round_bg_trang">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/hinh1_118"
                            android:layout_width="wrap_content"
                            android:layout_height="170dp"
                            app:cardCornerRadius="10dp"
                            app:cardElevation="0dp">

                            <ImageView
                                android:layout_width="220dp"
                                android:layout_height="160dp"
                                android:background="@drawable/ic_ch2"
                                android:scaleType="centerCrop"
                                tools:ignore="ContentDescription" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/haichau1_118"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hinh1_118"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:text="Quận Hải Châu"
                            android:textColor="#343434"
                            android:textSize="18dp"></TextView>

                        <TextView
                            android:id="@+id/nuithanh_118"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/haichau1_118"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:text="01 Núi Thành"
                            android:textColor="#343434"
                            android:textSize="20dp"
                            android:textStyle="bold"></TextView>
                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/quan2_118"
                        android:layout_width="wrap_content"
                        android:layout_height="250dp"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@id/quan1_118"
                        android:background="@drawable/round_bg_trang">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/hinh2_118"
                            android:layout_width="wrap_content"
                            android:layout_height="170dp"
                            app:cardCornerRadius="10dp"
                            app:cardElevation="0dp">

                            <ImageView
                                android:layout_width="220dp"
                                android:layout_height="160dp"
                                android:background="@drawable/ic_ch2"
                                android:scaleType="centerCrop"
                                tools:ignore="ContentDescription" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/thanhkhe1_118"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hinh2_118"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:text="Quận Thanh Khê"
                            android:textColor="#343434"
                            android:textSize="18dp"></TextView>

