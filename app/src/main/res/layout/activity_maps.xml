                            android:layout_marginTop="3dp"
                            android:text="09 Triệu Nữ Vương"
                            android:textColor="#343434"
                            android:textSize="20dp"
                            android:textStyle="bold"></TextView>
                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/quan5_118"
                        android:layout_width="wrap_content"
                        android:layout_height="250dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_toRightOf="@id/quan4_118"
                        android:background="@drawable/round_bg_trang">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/hinh5_118"
                            android:layout_width="wrap_content"
                            android:layout_height="170dp"
                            app:cardCornerRadius="10dp"
                            app:cardElevation="0dp">

                            <ImageView
                                android:layout_width="220dp"
                                android:layout_height="160dp"
                                android:background="@drawable/ic_ch2"
                                android:scaleType="centerCrop"
                                tools:ignore="ContentDescription" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/haichau4_118"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hinh5_118"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:text="Quận Hải Châu"
                            android:textColor="#343434"
                            android:textSize="18dp"></TextView>

                        <TextView
                            android:id="@+id/nguyenchithanh_118"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/haichau4_118"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:text="80A Nguyễn Chí Thanh"
                            android:textColor="#343434"
                            android:textSize="20dp"
                            android:textStyle="bold"></TextView>
                    </RelativeLayout>
                </RelativeLayout>
            </HorizontalScrollView>
        </fragment>

    </LinearLayout>


    <ImageView
        android:id="@+id/img_back_home_4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_button_back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />
</androidx.constraintlayout.widget.ConstraintLayout>